{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFBATCH14"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"SQL_Source_ls_1st_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SQL_Source_ls_1st'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageaccountclasss.dfs.core.windows.net/"
		},
		"SQL_Source_ls_1st_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserverclass14.database.windows.net"
		},
		"SQL_Source_ls_1st_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "azure_class_sql_pravin_1st"
		},
		"SQL_Source_ls_1st_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "pravin_azure_class_sql_login"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFBATCH14_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"SQL_Source_ls_1st_password": {
						"value": "[parameters('SQL_Source_ls_1st_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"SQL_Source_ls_1st_properties_typeProperties_server": {
						"value": "[parameters('SQL_Source_ls_1st_properties_typeProperties_server')]"
					},
					"SQL_Source_ls_1st_properties_typeProperties_database": {
						"value": "[parameters('SQL_Source_ls_1st_properties_typeProperties_database')]"
					},
					"SQL_Source_ls_1st_properties_typeProperties_userName": {
						"value": "[parameters('SQL_Source_ls_1st_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ADFBATCH14_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADFBATCH14_ArmTemplate_0')]"
			]
		}
	]
}